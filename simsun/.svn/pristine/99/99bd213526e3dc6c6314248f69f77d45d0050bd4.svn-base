<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/login_background" 
    android:id="@+id/root">

    <RelativeLayout
        android:id="@+id/top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/top_background" >

        <ImageView
            android:id="@+id/back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:background="@drawable/jsk_fh"
            android:onClick="btnClick" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:background="@drawable/qianfashouquan_03" />

        <ImageView
            android:id="@+id/cz"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/jsk_cz"
            android:onClick="btnClick" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/jyxh_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/top"
        android:layout_marginTop="5dp"
        android:gravity="center"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/qianfashouquan_jyxh"
            android:textColor="#33ccff" />

        <TextView
            android:id="@+id/jyxh"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#33ccff" />
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/jyxh_layout"
        android:layout_margin="10dp"
        android:background="@drawable/klxg_background" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" 
            android:layout_marginTop="20dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/txt_img1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/qianfashenqing_07" />

                <TextView
                    android:id="@+id/skgs"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="60dp"
                    android:layout_toRightOf="@+id/txt_img1"
                    android:singleLine="true"
                    android:layout_marginLeft="10dp" 
                    android:textColor="#000000"
                    android:textSize="15dp"
                    android:layout_centerInParent="true"
                    />
                

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/txt_img2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/qianfashenqing_11" />

                <TextView
                    android:id="@+id/skzh"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="60dp"
                    android:layout_toRightOf="@+id/txt_img2"
                    android:singleLine="true"
                    android:textColor="#000000"
                    android:layout_centerInParent="true"
                    android:textSize="15dp" 
                    android:layout_marginLeft="10dp"
                    />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/txt_img3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/qianfashenqing_15" />

                <TextView
                    android:id="@+id/fkzh"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="60dp"
                    android:layout_toRightOf="@+id/txt_img3"
                    android:singleLine="true" 
                    android:textColor="#000000"
                     android:layout_centerInParent="true"
                    android:textSize="15dp"
                    android:layout_marginLeft="10dp"
                    />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/txt_img4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/qianfashenqing_18" />

                <TextView
                    android:id="@+id/fkje"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="60dp"
                    android:layout_toRightOf="@+id/txt_img4"
                    android:singleLine="true"
                    android:textColor="#000000"
                    android:textSize="15dp"
                     android:layout_centerInParent="true"
                     android:layout_marginLeft="10dp"
                     />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/txt_img5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/qianfashenqing_20" />

                <TextView
                    android:id="@+id/jyrq"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="60dp"
                    android:layout_toRightOf="@+id/txt_img5"
                    android:singleLine="true"
                    android:textColor="#000000"
                     android:layout_centerInParent="true"
                    android:textSize="15dp" 
                    android:layout_marginLeft="10dp"/>

            </RelativeLayout>
            
                        <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:gravity="center"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/txt_img5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/qianfashouquan_24" />

                <TextView
                    android:id="@+id/ywzl"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="60dp"
                    android:layout_toRightOf="@+id/txt_img5"
                    android:singleLine="true"
                    android:textColor="#000000"
                     android:layout_centerInParent="true"
                    android:textSize="15dp" 
                    android:layout_marginLeft="10dp"/>

            </RelativeLayout>
            

        </LinearLayout>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/duanxintongzhi_txt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/content"
        android:gravity="center" 
        android:layout_marginTop="20dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/qianfashouquan_28" />
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/tx_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/duanxintongzhi_txt"
        android:layout_margin="10dp"
        android:background="@drawable/checkbox_backgroudn" >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="30dp"
            android:background="@drawable/qianfashenqing_27" />

        <CheckBox
            android:id="@+id/tixing"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="30dp"
            android:checked="true" 
           	/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" 
        android:layout_marginBottom="60dp">

        <ImageView
            android:id="@+id/ok"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:background="@drawable/make_auth_ok" 
            android:layout_marginLeft="15dp"
            android:onClick="btnClick"/>
        
        <ImageView
            android:id="@+id/no"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:background="@drawable/make_auth_no"
             android:layout_centerInParent="true"
             android:onClick="btnClick"
            />

        <ImageView
            android:id="@+id/wait"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:background="@drawable/make_auth_wait" 
            android:layout_marginRight="15dp"
            android:onClick="btnClick"/>
    </RelativeLayout>

</RelativeLayout>